# Environment
DEV_MODE="true" # Set to false for production with Supabase

# Logging
API_DEBUG="true"
LOG_LEVEL="INFO"
LOG_FORMAT="json"

# Security
CORS_ORIGINS="*"
API_KEY_REQUIRED="true"
API_KEY="ai-spine-dev-key-12345"

# Performance
MAX_CONCURRENT_EXECUTIONS="10"
EXECUTION_TIMEOUT="300"
HEALTH_CHECK_INTERVAL="30"

# Monitoring
ENABLE_METRICS="true"
METRICS_PORT="9090"

# Supabase
SUPABASE_URL="https://url.supabase.co"
SUPABASE_SERVICE_KEY="sb_secret_key"
SUPABASE_ANON_KEY="sb_publishable_key"